{
  "name": "@lovable/image-service",
  "version": "1.0.0",
  "description": "Automatic image integration service for generated applications using Unsplash API",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "require": "./src/index.ts",
      "types": "./src/index.ts"
    }
  },
  "files": [
    "src",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "test": "node src/test.js",
    "demo": "node src/demo.js",
    "validate": "node -c src/index.ts",
    "dev": "node --watch src/test.js"
  },
  "keywords": [
    "images",
    "unsplash",
    "automatic",
    "generator",
    "optimization",
    "responsive",
    "credits",
    "domain-specific",
    "placeholders"
  ],
  "author": "Lovable",
  "license": "MIT",
  "dependencies": {
    "unsplash-js": "^7.0.19",
    "node-fetch": "^3.3.2"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/lovable-dev/app-generator.git",
    "directory": "packages/@lovable/image-service"
  },
  "bugs": {
    "url": "https://github.com/lovable-dev/app-generator/issues"
  },
  "homepage": "https://github.com/lovable-dev/app-generator/tree/main/packages/@lovable/image-service#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "funding": {
    "type": "github",
    "url": "https://github.com/sponsors/lovable-dev"
  }
}